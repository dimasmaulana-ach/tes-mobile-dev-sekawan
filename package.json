{
  "name": "mobileapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "reset": "react-native start --reset-cache",
    "pod": "cd ios && pod install && cd ..",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build-staging": "react-native run-android --variant=stagingdebug --appIdSuffix=staging",
    "build-staging-release": "react-native run-android --variant=stagingrelease --appIdSuffix=staging",
    "build-dev": "react-native run-android --variant=developmentdebug --appIdSuffix=dev",
    "build-dev-release": "react-native run-android --variant=developmentrelease --appIdSuffix=dev",
    "build-prod": "react-native run-android --variant=productiondebug",
    "build-prod-release": "react-native run-android --variant=productionrelease",
    "apk-dev": "cd android && ./gradlew clean && ./gradlew assembleDevRelease",
    "apk-staging": "cd android && ./gradlew clean && ./gradlew assembleStagingRelease",
    "apk-prod": "cd android && ./gradlew clean && ./gradlew assembleProdRelease",
    "aab-dev": "cd android && ./gradlew clean && ./gradlew bundleDevRelease",
    "aab-staging": "cd android && ./gradlew clean && ./gradlew bundleStagingRelease",
    "aab-prod": "cd android && ./gradlew clean && ./gradlew bundleProdRelease",
    "test": "jest"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "5.3.1",
    "@ui-kitten/metro-config": "5.3.1",
    "axios": "^1.4.0",
    "i18next": "^23.4.4",
    "react": "18.2.0",
    "react-i18next": "^13.0.3",
    "react-native": "0.71.11",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-mmkv": "^2.10.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-svg": "^13.11.0",
    "react-native-svg-transformer": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
